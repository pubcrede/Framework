      <dependentAssembly>
        <assemblyIdentity name="Genesys.Extras.Core" publicKeyToken="d7d438e664f27f82" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.17.7.0" newVersion="3.17.7.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Genesys.Extensions.Core" publicKeyToken="d7d438e664f27f82" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.17.7.0" newVersion="3.17.7.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>